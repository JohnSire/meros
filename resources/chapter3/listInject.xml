<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="listBean" class="com.javass.spring.chapter3.bean.ListTestBean">
		<property name="values">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</list>
		</property>
	</bean>

	<bean id="setBean" class="com.javass.spring.chapter3.bean.CollectionTestBean">
		<property name="values">
			<set>
				<value>1</value>
				<value>2</value>
				<value>3</value>
			</set>
		</property>
	</bean>

	<bean id="mapBean" class="com.javass.spring.chapter3.bean.MapTestBean">
		<property name="values">
			<map key-type="java.lang.String" value-type="java.lang.String">
				<entry>
					<key>
						<value>1</value>
					</key>
					<value>11</value>
				</entry>

				<entry key="2" value="22" />
			</map>
		</property>
	</bean>
</beans>
